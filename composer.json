{
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "symfony/asset": "^5.0",
        "hubspot/hubspot-php": "^2.0",
        "yoast/wordpress-seo-premium": "^14.2"
    },
    "repositories": {
        "my-yoast": {
            "type": "composer",
            "url": "https://my.yoast.com/packages/"
        }
    },
    "extra": {
        "installer-paths": {
